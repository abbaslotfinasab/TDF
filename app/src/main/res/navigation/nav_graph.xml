<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.utechia.tdf.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/teaBoyFragment"
        android:name="com.utechia.tdf.refreshment.RefreshmentsFragment"
        android:label="fragment_tea_boy"
        tools:layout="@layout/fragment_tea_boy" >
        <action
            android:id="@+id/action_teaBoyFragment_to_createRefreshmentFragment"
            app:destination="@id/createRefreshmentFragment" />
        <action
            android:id="@+id/action_teaBoyFragment_to_cartFragment"
            app:destination="@id/cartFragment" />
        <action
            android:id="@+id/action_teaBoyFragment_to_favoriteFragment"
            app:destination="@id/favoriteFragment" />
        <action
            android:id="@+id/action_teaBoyFragment_to_previousOrdersFragment"
            app:destination="@id/previousOrdersFragment" />
    </fragment>
    <fragment
        android:id="@+id/reservationFragment"
        android:name="com.utechia.tdf.reservation.ReservationFragment"
        android:label="fragment_reservation"
        tools:layout="@layout/fragment_reservation" >
        <action
            android:id="@+id/action_reservationFragment_to_createReservationFragment"
            app:destination="@id/createReservationFragment" />
        <action
            android:id="@+id/action_reservationFragment_to_reservationDetails"
            app:destination="@id/reservationDetails" />
        <action
            android:id="@+id/action_reservationFragment_self"
            app:destination="@id/reservationFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.utechia.tdf.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.utechia.tdf.more.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" />
    <fragment
        android:id="@+id/createReservationFragment"
        android:name="com.utechia.tdf.reservation.CreateReservationFragment"
        android:label="fragment_create_reservation"
        tools:layout="@layout/fragment_create_reservation" >
        <action
            android:id="@+id/action_createReservationFragment_self"
            app:destination="@id/createReservationFragment" />
        <argument
            android:name="month_id"
            app:argType="integer" />
        <argument
            android:name="day_id"
            app:argType="integer" />
        <action
            android:id="@+id/action_createReservationFragment_to_reservationDetails"
            app:destination="@id/reservationDetails" />
    </fragment>
    <fragment
        android:id="@+id/reservationDetails"
        android:name="com.utechia.tdf.reservation.ReservationDetails"
        android:label="fragment_reservation_details"
        tools:layout="@layout/fragment_reservation_details" >
        <argument
            android:name="roomTitle"
            app:argType="string" />
        <argument
            android:name="currentDay"
            app:argType="integer" />
        <argument
            android:name="currentMonth"
            app:argType="integer" />
        <argument
            android:name="imageRoom"
            app:argType="string" />
        <argument
            android:name="time"
            app:argType="string" />
        <argument
            android:name="capacity"
            app:argType="integer" />
        <action
            android:id="@+id/action_reservationDetails_to_resultFragment"
            app:destination="@id/resultFragment" />
        <argument
            android:name="duration"
            app:argType="string" />
        <action
            android:id="@+id/action_reservationDetails_to_createReservationFragment"
            app:destination="@id/createReservationFragment" />
    </fragment>
    <fragment
        android:id="@+id/qrCodeFragment"
        android:name="com.utechia.tdf.qrcode.QrCodeFragment"
        android:label="fragment_qr_code"
        tools:layout="@layout/fragment_qr_code" />
    <fragment
        android:id="@+id/surveySystemFragment"
        android:name="com.utechia.tdf.more.SurveySystemFragment"
        android:label="fragment_survey_system"
        tools:layout="@layout/fragment_survey_system" />
    <fragment
        android:id="@+id/eventSystemFragment"
        android:name="com.utechia.tdf.more.EventSystemFragment"
        android:label="fragment_event_system"
        tools:layout="@layout/fragment_event_system" />
    <fragment
        android:id="@+id/ticketSystemFragment"
        android:name="com.utechia.tdf.more.TicketSystemFragment"
        android:label="fragment_ticket_system"
        tools:layout="@layout/fragment_ticket_system" >
        <action
            android:id="@+id/action_ticketSystemFragment_to_createTicketFragment"
            app:destination="@id/createTicketFragment" />
    </fragment>

    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.utechia.tdf.more.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" />

    <fragment
        android:id="@+id/createTicketFragment"
        android:name="com.utechia.tdf.more.CreateTicketFragment"
        android:label="fragment_create_ticket"
        tools:layout="@layout/fragment_create_ticket" />
    <fragment
        android:id="@+id/permissionFragment"
        android:name="com.utechia.tdf.more.PermissionFragment"
        android:label="fragment_permission"
        tools:layout="@layout/fragment_permission" />
    <dialog
        android:id="@+id/resultFragment"
        android:name="com.utechia.tdf.reservation.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" >
        <action
            android:id="@+id/action_resultFragment_to_reservationFragment"
            app:destination="@id/reservationFragment" />
    </dialog>
    <fragment
        android:id="@+id/createRefreshmentFragment"
        android:name="com.utechia.tdf.refreshment.CreateRefreshmentFragment"
        android:label="fragment_create_refreshment"
        tools:layout="@layout/fragment_create_refreshment" >
        <argument
            android:name="kind"
            app:argType="integer" />
        <action
            android:id="@+id/action_createRefreshmentFragment_to_cartFragment"
            app:destination="@id/cartFragment" />
        <action
            android:id="@+id/action_createRefreshmentFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
        <action
            android:id="@+id/action_createRefreshmentFragment_to_favoriteResultFragment"
            app:destination="@id/favoriteResultFragment" />
        <action
            android:id="@+id/action_createRefreshmentFragment_to_favoriteFragment"
            app:destination="@id/favoriteFragment" />
        <action
            android:id="@+id/action_createRefreshmentFragment_to_previousOrdersFragment"
            app:destination="@id/previousOrdersFragment" />
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.utechia.tdf.refreshment.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_cartFragment_to_orderResultFragment"
            app:destination="@id/orderResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.utechia.tdf.order.OrderFragment"
        android:label="fragment_order"
        tools:layout="@layout/fragment_order" >
        <action
            android:id="@+id/action_orderFragment_to_orderResultFragment"
            app:destination="@id/orderResultFragment" />
    </fragment>

    <dialog
        android:id="@+id/orderResultFragment"
        android:name="com.utechia.tdf.order.OrderResultFragment"
        android:label="fragment_order_result"
        tools:layout="@layout/fragment_order_result" >
        <action
            android:id="@+id/action_orderResultFragment_to_previousOrdersFragment"
            app:destination="@id/previousOrdersFragment" />
    </dialog>
    <dialog
        android:id="@+id/favoriteResultFragment"
        android:name="com.utechia.tdf.favorite.FavoriteResultFragment"
        android:label="fragment_favorite_result"
        tools:layout="@layout/fragment_favorite_result" >
        <action
            android:id="@+id/action_favoriteResultFragment_to_favoriteFragment"
            app:destination="@id/favoriteFragment" />
    </dialog>
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.utechia.tdf.favorite.FavoriteFragment"
        android:label="fragment_favorite"
        tools:layout="@layout/fragment_favorite" />
    <fragment
        android:id="@+id/previousOrdersFragment"
        android:name="com.utechia.tdf.order.PreviousOrdersFragment"
        android:label="fragment_previous_orders"
        tools:layout="@layout/fragment_previous_orders" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.utechia.tdf.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login"/>

    <fragment
        android:id="@+id/waitingFragment"
        android:name="com.utechia.tdf.login.WaitingFragment"
        android:label="fragment_waiting"
        tools:layout="@layout/fragment_waiting" >

        <deepLink
            android:id="@+id/deepLink"
            app:uri="https://auth.tdf.gov.sa:4002/?code={code}" />

        <argument
            android:name="code"
            app:argType="string"
            app:nullable="true" />

        <action
            android:id="@+id/action_waitingFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_waitingFragment_to_authenticationFragment"
            app:destination="@id/authenticationFragment" />

    </fragment>

    <dialog
        android:id="@+id/authenticationFragment"
        android:name="com.utechia.tdf.login.AuthenticationFragment"
        android:label="fragment_authentication"
        tools:layout="@layout/fragment_authentication" >

        <action
            android:id="@+id/action_authenticationFragment_to_loginFragment"
            app:destination="@id/loginFragment" />

    </dialog>

</navigation>